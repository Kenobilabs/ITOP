        <height>600</height>
        <linkType>url</linkType>
        <masterLabel>Air, Rail or Bus Travel Proposal</masterLabel>
        <openType>sidebar</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>/apex/MERC_TravelProposals?id={!Meeting_Participant_MERC__c.Id}</url>
    </webLinks>
</CustomObject>
